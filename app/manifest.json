{
    "version": "1.0.10",
    "name": "__MSG_name__",
    "short_name": "__MSG_name__",
    "description": "__MSG_description__",
    "manifest_version": 2,
    "default_locale": "en",
    "minimum_chrome_version": "36.0",
    "app": {
        "background": {
            "scripts": [ "scripts/background.js" ]
        }
    },
    "permissions": [
        "audio",
        "audioCapture",
        "videoCapture",
        "notifications",
        "fileSystem",
        "webview"
    ],
    "icons": {
        "16": "images/16.png",
        "19": "images/19.png",
        "38": "images/38.png",
        "128": "images/128.png",
        "256": "images/256.png",
        "512": "images/512.png"
    }
}
